<LinearLayout android:id="@+id/keyboardset_btn_layout"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/keyboardset_back"
    android:weightSum="1"
    xmlns:android="http://schemas.android.com/apk/res/android">

        <Space
            android:layout_width="1px"
            android:layout_height="0px"
            android:layout_weight="0.83"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.067"
            android:weightSum="1">

            <Space
                android:layout_width="0px"
                android:layout_height="1px"
                android:layout_weight="0.064"/>

            <ImageButton
                android:onClick="onClick"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.429"
                android:id="@+id/keyboardset_btn1"
                android:background="@drawable/keyboardset_btn1"/>

            <Space
                android:layout_width="0px"
                android:layout_height="1px"
                android:layout_weight="0.014"/>

            <ImageButton
                android:onClick="onClick"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.429"
                android:id="@+id/keyboardset_btn2"
                android:background="@drawable/keyboardset_btn2"/>

            <Space
                android:layout_width="0px"
                android:layout_height="1px"
                android:layout_weight="0.064"/>

        </LinearLayout>

        <Space
            android:layout_width="1px"
            android:layout_height="0px"
            android:layout_weight="0.008"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.067"
            android:weightSum="1">

            <Space
                android:layout_width="0px"
                android:layout_height="1px"
                android:layout_weight="0.064"/>

            <ImageButton
                android:onClick="onClick"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.872"
                android:id="@+id/keyboardset_btn3"
                android:background="@drawable/keyboardset_btn3"/>

            <Space
                android:layout_width="0px"
                android:layout_height="1px"
                android:layout_weight="0.064"/>
        </LinearLayout>

        <Space
            android:layout_width="1px"
            android:layout_height="0px"
            android:layout_weight="0.028"/>

    </LinearLayout>